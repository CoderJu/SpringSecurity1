<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC

        "-//Hibernate/Hibernate Mapping DTD 5.0//EN"

        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.eleven.model">
    <class name="Profileid" table="profile">
        <id name="id" type="java.lang.Integer" column="id">
            <generator class="native"/>
        </id>
        <property name="type" type="java.lang.String" column="type"/>
        <set name="userToProfileidHashSet" table="user_profile">
            <key column="user_profileid"></key>
            <one-to-many class="UserToProfileid"/>
        </set>
    </class>

</hibernate-mapping>